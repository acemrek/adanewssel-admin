<ivysettings>
	<properties file="${ivy.settings.dir}/ivy.properties" />
	<settings defaultResolver="default-chain" />
	<resolvers>
		<chain name="default-chain">
			<ibiblio name="springsource-release" root="http://maven.springframework.org/release" m2compatible="true" />
			<ibiblio name="springsource-external" root="http://repository.springsource.com/maven/bundles/external" m2compatible="true" />
			<ibiblio name="springsource-milestone" root="http://maven.springframework.org/milestone" m2compatible="true" />
			<ibiblio name="JBoss repository" root="https://repository.jboss.org/nexus/content/groups/public" m2compatible="true" />
			<ibiblio name="jvnet-nexus-releases" root="https://maven.java.net/content/repositories/releases/" m2compatible="true" />
			<ibiblio name="jvnet-nexus-snapshots" root="https://maven.java.net/content/repositories/snapshots/" m2compatible="true" />
			<ibiblio name="prime-repo" root="http://repository.primefaces.org" m2compatible="true" />
			<ibiblio name="jets3t" root="http://www.jets3t.org/maven2" m2compatible="true" />
			<ibiblio name="sun" root="http://download.java.net/maven/2/" m2compatible="true" />
			<ibiblio name="ibiblio" m2compatible="true" />
		</chain>
	</resolvers>

</ivysettings>