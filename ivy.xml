        <dependency org="org.apache.httpcomponents" name="httpcore" rev="4.2" conf="webinfBinaries->default" />
        <dependency org="org.apache.httpcomponents" name="httpclient" rev="4.1.3" conf="webinfBinaries->default" />
        <dependency org="log4j" name="log4j" rev="1.2.16" conf="webinfBinaries->default" />
        <dependency org="org.slf4j" name="slf4j-log4j12" rev="1.6.1" conf="webinfBinaries->default" />
        <dependency org="org.imgscalr" name="imgscalr-lib" rev="4.2" conf="webinfBinaries->default" />
        
        <dependency org="javax.servlet" name="servlet-api" rev="2.5" conf="compileBinaries->default"/>
        <dependency org="org.apache.tomcat" name="annotations-api" rev="6.0.18" conf="compileBinaries->default"/>
        
        <dependency org="org.primefaces.extensions" name="primefaces-extensions" rev="0.5.1" conf="sources->sources" />
        <dependency org="org.primefaces" name="primefaces" rev="3.3.1" conf="sources->sources" />
        <dependency org="org.glassfish" name="javax.faces" rev="2.1.10" conf="sources->sources" />
        
        <exclude org="aopalliance" conf="*" />
    </dependencies>

</ivy-module>