        <dependency org="net.java.dev.jets3t" name="jets3t" rev="0.9.0" conf="webinfBinaries->default">
        	<exclude org="org.apache.httpcomponents" />
        </dependency>
        
        <dependency org="mysql" name="mysql-connector-java" rev="5.1.20" conf="webinfBinaries->default" />
        
        <dependency org="org.hibernate" name="hibernate-core" rev="3.6.10.Final" conf="webinfBinaries->default">
        	<exclude org="commons-collections" name="commons-collections"/>
        </dependency>
        
        <dependency org="commons-beanutils" name="commons-beanutils" rev="1.8.3" conf="webinfBinaries->default" />
