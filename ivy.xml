        <dependency org="org.springframework" name="spring-orm" rev="${org.springframework.version}" conf="webinfBinaries->default" />
        <dependency org="org.springframework" name="spring-oxm" rev="${org.springframework.version}" conf="webinfBinaries->default" />
        <dependency org="org.springframework" name="spring-web" rev="${org.springframework.version}" conf="webinfBinaries->default" />
        
        <dependency org="org.aopalliance" name="com.springsource.org.aopalliance" rev="1.0.0" conf="webinfBinaries->default" />
        
        <dependency org="org.springframework.security" name="spring-security-core" rev="${org.springframework.security.version}" conf="webinfBinaries->default" />
        <dependency org="org.springframework.security" name="spring-security-web" rev="${org.springframework.security.version}" conf="webinfBinaries->default" />
