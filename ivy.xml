        <dependency org="commons-fileupload" name="commons-fileupload" rev="1.2.2" conf="webinfBinaries->default" />
        <dependency org="commons-io" name="commons-io" rev="2.3" conf="webinfBinaries->default" />
        <dependency org="commons-collections" name="commons-collections" rev="3.2.1" conf="webinfBinaries->default" />
        
