    </configurations>

    <dependencies>
        <dependency org="org.springframework" name="spring-core" rev="${org.springframework.version}" conf="webinfBinaries->default" />
        <dependency org="org.springframework" name="spring-expression" rev="${org.springframework.version}" conf="webinfBinaries->default" />
        <dependency org="org.springframework" name="spring-beans" rev="${org.springframework.version}" conf="webinfBinaries->default" />
        <dependency org="org.springframework" name="spring-aop" rev="${org.springframework.version}" conf="webinfBinaries->default" />
