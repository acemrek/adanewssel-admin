<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
	<application>

		<el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>

		<locale-config>
			<supported-locale>tr</supported-locale>
		</locale-config>

		<message-bundle>com.ac.newsadmin.i18n.resources</message-bundle>

		<resource-bundle>
			<base-name>com.ac.newsadmin.i18n.messages</base-name>
			<var>msg</var>
		</resource-bundle>

		<resource-bundle>
			<base-name>com.ac.newsadmin.i18n.errors</base-name>
			<var>err</var>
		</resource-bundle>

		<resource-bundle>
			<base-name>app</base-name>
			<var>app</var>
		</resource-bundle>


	</application>
</faces-config>
